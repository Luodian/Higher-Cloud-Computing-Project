<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>LogInServlet</servlet-name>
        <servlet-class>LogInServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CheckEmailExistServlet</servlet-name>
        <servlet-class>CheckEmailExistServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetMachineInfoServlet</servlet-name>
        <servlet-class>GetMachineInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetMachineInfoWithTaskServlet</servlet-name>
        <servlet-class>GetMachineInfoWithTaskServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetOnlineMachineInfoServlet</servlet-name>
        <servlet-class>GetOnlineMachineInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MachineOfflineServlet</servlet-name>
        <servlet-class>MachineOfflineServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyUserInfoServlet</servlet-name>
        <servlet-class>ModifyUserInfoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateOnlineTimeServlet</servlet-name>
        <servlet-class>UpdateOnlineTimeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LogInServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CheckEmailExistServlet</servlet-name>
        <url-pattern>/check_email_exist</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetMachineInfoServlet</servlet-name>
        <url-pattern>/get_machine_info</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetMachineInfoWithTaskServlet</servlet-name>
        <url-pattern>/get_machine_info_with_task</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>GetOnlineMachineInfoServlet</servlet-name>
        <url-pattern>/get_online_machine_info</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>MachineOfflineServlet</servlet-name>
        <url-pattern>/machine_offline</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ModifyUserInfoServlet</servlet-name>
        <url-pattern>/modify_user_info</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpdateOnlineTimeServlet</servlet-name>
        <url-pattern>/update_online_time</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>client.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Active</servlet-name>
        <servlet-class>client.Active</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Active</servlet-name>
        <url-pattern>/Active</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetUserInfo</servlet-name>
        <servlet-class>client.GetUserInfo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetUserInfo</servlet-name>
        <url-pattern>/GetUserInfo</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>client.Register</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ImgUpload</servlet-name>
        <servlet-class>client.ImgUpload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ImgUpload</servlet-name>
        <url-pattern>/ImgUpload</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UploadIP</servlet-name>
        <servlet-class>client.UploadIP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadIP</servlet-name>
        <url-pattern>/UploadIP</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UploadPK</servlet-name>
        <servlet-class>client.UploadPK</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadPK</servlet-name>
        <url-pattern>/UploadPK</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SubmitTask</servlet-name>
        <servlet-class>client.SubmitTask</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitTask</servlet-name>
        <url-pattern>/SubmitTask</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>TaskFinish</servlet-name>
        <servlet-class>client.TaskFinish</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TaskFinish</servlet-name>
        <url-pattern>/TaskFinish</url-pattern>
    </servlet-mapping>
