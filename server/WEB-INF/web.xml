        <servlet-name>PwdResetVal</servlet-name>
        <servlet-class>client.PwdResetVal</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PwdResetVal</servlet-name>
        <url-pattern>/PwdResetVal</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ResetPwd</servlet-name>
        <servlet-class>client.ResetPwd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResetPwd</servlet-name>
        <url-pattern>/ResetPwd</url-pattern>
    </servlet-mapping>

</web-app>